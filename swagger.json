  "paths": {
    "/student": {
      "description": "Student resource",
      "get": {
        "description": "Operation to fetch student",
        "parameters": [
          {
            "in": "query",
            "name": "studentname",
            "required": true,
            "schema": {
              "type": "string",
              "example": "Rob"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "sucess",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "properties": {
                      "studentID": {
                        "type": "integer",
                        "example": 3
                      },
                      "studentname": {
                        "type": "string",
                        "example": "Mark"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}